{
  "name": "be",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aragon/ui": "^1.2.0",
    "@web3-react/core": "^6.0.2",
    "@web3-react/injected-connector": "^6.0.3",
    "bn.js": "^5.1.1",
    "dayjs": "^1.8.14",
    "ethers": "^4.0.42",
    "file-saver": "^2.0.1",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "js-sha3": "^0.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "styled-components": "^4.4.1",
    "subscriptions-transport-ws": "^0.9.16",
    "urql": "^1.6.3"
  },
  "scripts": {
    "start": "npm run sync-assets && react-app-rewired start",
    "start:rpc": "REACT_APP_CHAIN_ID=1337 npm run start ",
    "start:mainnet": "REACT_APP_CHAIN_ID=1 npm run start",
    "start:ropsten": "REACT_APP_CHAIN_ID=3 npm run start",
    "start:rinkeby": "REACT_APP_CHAIN_ID=4 npm run start",
    "build": "npm run sync-assets && react-app-rewired build",
    "build:rpc": "REACT_APP_CHAIN_ID=1337 npm run build",
    "build:mainnet": "REACT_APP_CHAIN_ID=1 npm run build",
    "build:ropsten": "REACT_APP_CHAIN_ID=3 npm run build",
    "build:rinkeby": "REACT_APP_CHAIN_ID=4 npm run build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "optimize": "npm run optimize:svg",
    "optimize:svg": "find ./src -name *.svg -exec svgo --config '{ \"plugins\": [ { \"removeDesc\": {\"removeAny\": true} }, { \"removeTitle\": true }, { \"removeViewBox\": false } ] }' {} \\;",
    "sync-assets": "copy-aragon-ui-assets ./public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@urql/devtools": "^0.1.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.10.6",
    "customize-cra": "^0.9.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "subscriptions-transport-ws": "^0.9.16",
    "svgo": "^1.3.2"
  }
}
